{
    "name": "ray/di",
    "type": "library",
    "description": "Guice style annotation-driven dependency injection framework",
    "keywords": [
        "dependency injection",
        "di",
        "container",
        "guice",
        "annotation",
        "ioc",
        "aop",
        "method interception",
        "jsr"
    ],
    "homepage": "https://github.com/koriym/Ray.Di",
    "version": "1.0.0-beta1",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "doctrine/common": "2.3.*-dev",
        "Ray/Aop": ">=1.0.*-dev",
        "Aura/Di": ">=1.0.*-dev"
    },
    "replace": {
        "Ray/Di": "self.version"
    },
    "autoload": {
        "psr-0": {
            "Ray\\Di": "src/",
            "Aura\\Di": "vendor/Aura/Di/src/" 
        }
    },
    "minimum-stability": "dev"
}